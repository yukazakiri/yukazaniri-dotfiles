{
   "action": {
      "default_icon": {
         "128": "/assets/protonpass-icon-128.png",
         "16": "/assets/protonpass-icon-16.png",
         "32": "/assets/protonpass-icon-32.png",
         "48": "/assets/protonpass-icon-48.png"
      },
      "default_popup": "popup.html"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "orchestrator.js" ],
      "matches": [ "https://*/*", "http://*/*" ],
      "run_at": "document_end"
   }, {
      "all_frames": false,
      "exclude_matches": [ "https://pass.proton.me/*", "https://pass.proton.dev/*", "https://pass.proton.black/*", "https://pass.proton.pink/*", "https://pass.protontech.ch/*" ],
      "js": [ "webauthn.js" ],
      "matches": [ "https://*/*", "http://*/*" ],
      "run_at": "document_start",
      "world": "MAIN"
   } ],
   "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
   },
   "description": "Free and fully encrypted password manager to keep your logins and passwords safe.",
   "externally_connectable": {
      "matches": [ "https://account.proton.me/*", "https://pass.proton.me/*" ]
   },
   "host_permissions": [ "https://*/*", "http://*/*" ],
   "icons": {
      "128": "/assets/protonpass-icon-128.png",
      "16": "/assets/protonpass-icon-16.png",
      "32": "/assets/protonpass-icon-32.png",
      "48": "/assets/protonpass-icon-48.png"
   },
   "key": "",
   "manifest_version": 3,
   "minimum_chrome_version": "102",
   "name": "Proton Pass: Free Password Manager",
   "optional_permissions": [ "privacy", "webRequestAuthProvider", "clipboardRead", "clipboardWrite" ],
   "options_ui": {
      "page": "settings.html"
   },
   "permissions": [ "activeTab", "alarms", "scripting", "storage", "unlimitedStorage", "webRequest", "offscreen" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.33.2",
   "web_accessible_resources": [ {
      "matches": [ "https://*/*", "http://*/*" ],
      "resources": [ "dropdown.html", "notification.html", "elements.js", "assets/*.csv", "assets/*.png", "assets/*.svg", "assets/*.woff", "assets/*.woff2" ]
   }, {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "*.wasm" ]
   } ]
}
